{
    // Required; must match the id of the cheat sheet's IA page
    // For example, https://duck.co/ia/view/vim_cheat_sheet
    "id": "vim_cheat_sheet", 

    // Required; displayed as title of AnswerBar
    "name": "Firefox",

    // Optional; displayed as subtitle of AnswerBar
    "description": "Web Browser", 
    
    // Required if cheat sheet has a source; useful to link users to further information.
    // Displayed at bottom of AnswerBar, favicon shown automatically
    "metadata": { 
        "sourceName": "More at Firefoxshortcuts",
        "sourceUrl": "https://support.mozilla.org/en-US/kb/keyboard-shortcuts-perform-firefox-tasks-quickly" // Should be SSL if possible
    },

    // Required; controls which sections appear and in what order
    "section_order": [
        "Tools",
        "Bookmarks",
        "Tabs/Windows"
        // ...additional sections
        //"History",
    ],

    // Required; section names must match those in section_order in order to appear
    "sections": {
        "Tools": [
            {   
                "key": "[Ctrl] [Shift] [i]", 
                "val": "Toggle Developer Tools"
            },
            {
                "key": "[Ctrl] [Shift] [k]",
                "val": "Web Console"
            }
            // ...additional entries
        ],        
        "Bookmarks": [
            {
                "key": "[Ctrl] [d]",
                "val": "Bookmark This Page"               
            }, 
            {
                "key": "[Ctrl] [b]",
                "val": "Bookmarks Sidebar"
            }
            // ...additional entries
        ],  
        // ...additional sections
        "Tabs/Windows": [
            {
                "key": "[Ctrl] [t]",
                "val": "New Tab"
            },
            {
                "key": "[Ctrl] [n]",
                "val": "New Window"
            },
            {
                "key": "[Ctrl] [Tab]",
                "val": "Next Tab"
            }
        ]
    }
}